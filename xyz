 <section data-variation="v0-doc" title="">
  <a class="f-a00v0" href="/mobstor/publication_attachments/main.pdf">
   Download
  </a>
 </section>
 <!--<section title="" data-variation="v1-doc-example">
                                <div class="f-a02v1">
